    "script": {
        "dev": [
            {
                "id": "6838D757DAD4491CB74E47454953B434",
                "tag": "dev",
                "type": "shell",
                "name": "install_poetry",
                "description": "Poetry installation script",
                "shebang": null,
                "body": [
                    "sudo apt install curl git",
                    "curl -sSL https://raw.githubusercontent.com/python-poetry/poetry/master/get-poetry.py | python3 -"
                ]
            },
            {
                "id": "DC7EC742C7BA4AD5AE0A393AB6398A8B",
                "tag": "dev",
                "type": "shell",
                "name": "install_asdf",
                "description": "ASDF installation script",
                "shebang": null,
                "body": [
                    "sudo apt install curl git",
                    "git clone https://github.com/asdf-vm/asdf.git ~/.asdf --branch v0.10.2",
                    ". $HOME/.asdf/asdf.sh",
                    ". $HOME/.asdf/completions/asdf.bash",
                    "",
                    "echo '' >> ~/.bashrc",
                    "echo '# ASDF' >> ~/.bashrc",
                    "echo '. $HOME/.asdf/asdf.sh' >> ~/.bashrc",
                    "echo '. $HOME/.asdf/completions/asdf.bash' >> ~/.bashrc",
                    "",
                    "echo 'INSTALL: python'",
                    "asdf plugin-add python"
                ]
            }
        ],
        "system": [
            {
                "id": "1C6B2B554D53441FB8C98CC1C8979679",
                "tag": "system",
                "type": "shell",
                "name": "clean_linux",
                "description": "Script to clear linux memory",
                "shebang": null,
                "body": [
                    "sudo apt clean",
                    "sudo apt autoclean",
                    "sudo apt autoremove",
                    "sudo apt-get autoremove",
                    "sudo apt-get autoclean",
                    "sudo apt-get clean",
                    "journalctl --disk-usage",
                    "sudo journalctl --vacuum-time=7d",
                    "sudo du -sh /var/cache/apt",
                    "sudo du -sh ~/.cache/thumbnails",
                    "sudo rm -rf ~/.cache/thumbnails/*",
                    "sudo du -h /var/cache/apt/archives"
                ]
            }
        ],
        "django": [
            {
                "id": "0A50759C27B34C279668219F84972F91",
                "tag": "django",
                "type": "python3",
                "name": "sqlmigrate",
                "description": "Script to generate 'sqlmigrate' from Django framework",
                "shebang": null,
                "body": [
                    "from os import system",
                    "",
                    "module = input('Module: ')",
                    "migration_name = input('Migration Name: ')",
                    "system(f'python src/manage.py sqlmigrate {module} {migration_name}')"
                ]
            }
        ],
        "archlinux_init": [
            {
                "id": "1C7769FC36894E62861BEF81129B9FE5",
                "tag": "archlinux_init",
                "type": "shell",
                "name": "init_step_00",
                "body": [
                    "read -p 'Root Partition: ' root_partition",
                    "read -p 'Swap Partition: ' swap_partition",
                    "read -p 'EFI System Partition: ' efi_system_partition",
                    "",
                    "mkfs.ext4 /dev/$root_partition",
                    "mkswap /dev/$swap_partition",
                    "mkfs.fat -F 32 /dev/$efi_system_partition",
                    "",
                    "mount /dev/$root_partition /mnt",
                    "mount --mkdir /dev/$efi_system_partition /mnt/boot",
                    "swapon /dev/$swap_partition"
                ],
                "shebang": null,
                "description": "LIVE: Disk partitioning"
