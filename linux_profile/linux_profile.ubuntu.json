    "script": {
        "install": [
            {
                "id": "6838D757DAD4491CB74E47454953B434",
                "tag": "install",
                "type": "shell",
                "name": "poetry",
                "description": "Poetry installation script",
                "shebang": null,
                "body": [
                    "linuxp install -m package -i git --sudo",
                    "",
                    "curl -sSL https://install.python-poetry.org | python3 -"
                ]
            },
            {
                "id": "DC7EC742C7BA4AD5AE0A393AB6398A8B",
                "tag": "install",
                "type": "shell",
                "name": "asdf",
                "body": [
                    "linuxp install -m package -i git --sudo",
                    "",
                    "git clone https://github.com/asdf-vm/asdf.git ~/.asdf --branch v0.10.2",
                    ". $HOME/.asdf/asdf.sh",
                    ". $HOME/.asdf/completions/asdf.bash",
                    "",
                    "echo '. $HOME/.asdf/asdf.sh' >> ~/.bashrc",
                    "echo '. $HOME/.asdf/completions/asdf.bash' >> ~/.bashrc"
                ],
                "shebang": null,
                "description": "No description"
            },
            {
                "id": "28791AB64CA44A57A4508B18642EE631",
                "tag": "install",
                "type": "shell",
                "name": "vim",
                "body": [
                    "linuxp install -m package -i vim --sudo",
                    "linuxp install -m package -i curl --sudo",
                    "",
                    "if [ ! -n '/etc/vim' ]; then",
                    "    mkdir /etc/vim/",
                    "fi",
                    "",
                    "array=('vimrc', 'vimrc.local', '.vimrc.plug')",
                    "sudo curl -fLo /etc/vim/autoload/plug.vim --create-dirs https://raw.githubusercontent.com/junegunn/vim-plug/master/plug.vim",
                    "",
                    "for file in ${array[@]}; do",
                    "    touch /etc/vim/$file",
                    "    echo '$(linuxp list -m file -i $file --field body --print)' > /etc/vim/$file",
                    "done"
                ],
                "shebang": null,
                "description": "No description"
            }
        ],
        "system": [
            {
                "id": "1C6B2B554D53441FB8C98CC1C8979679",
                "tag": "system",
                "type": "shell",
                "name": "clean_linux",
                "description": "Script to clear linux memory",
                "shebang": null,
                "body": [
                    "sudo apt clean",
                    "sudo apt autoclean",
                    "sudo apt autoremove",
                    "sudo apt-get autoremove",
                    "sudo apt-get autoclean",
                    "sudo apt-get clean",
                    "journalctl --disk-usage",
                    "sudo journalctl --vacuum-time=7d",
                    "sudo du -sh /var/cache/apt",
                    "sudo du -sh ~/.cache/thumbnails",
                    "sudo rm -rf ~/.cache/thumbnails/*",
                    "sudo du -h /var/cache/apt/archives"
                ]
            }
        ],
        "django": []
    },
