    "script": {
        "init": [
            {
                "id": "EE959D7607694622B030BEEA1174AF56",
                "tag": "init",
                "type": "shell",
                "name": "step_01",
                "body": [
                    "read -p 'Root Partition: ' root_partition",
                    "read -p 'Swap Partition: ' swap_partition",
                    "read -p 'EFI System Partition: ' efi_system_partition",
                    "",
                    "mkfs.ext4 /dev/$root_partition",
                    "mkswap /dev/$swap_partition",
                    "mkfs.fat -F 32 /dev/$efi_system_partition",
                    "",
                    "mount /dev/$root_partition /mnt",
                    "mount --mkdir /dev/$efi_system_partition /mnt/boot",
                    "swapon /dev/$swap_partition"
                ],
                "shebang": null,
                "description": "LIVE: Disk partitioning"
            },
            {
                "id": "A48D2542AF3045888DC1242EF77C7666",
                "tag": "init",
                "type": "shell",
                "name": "step_02",
                "body": [
                    "linuxp install -m package -t config --sudo --group",
                    "linuxp install -m package -t python --sudo --group"
                ],
                "shebang": null,
                "description": "LIVE: Arch Linux PGP keyring"
            },
            {
                "id": "946356F798A64B7FADDD93DB351ADE92",
                "tag": "init",
                "type": "shell",
                "name": "step_03",
                "body": [
                    "pacstrap /mnt base linux linux-firmware nano vim dhcpcd"
                ],
                "shebang": null,
                "description": "LIVE: Installation of Base Packages"
            },
            {
                "id": "6C5C1A13EC434063983A80CC40EAB25C",
                "tag": "init",
                "type": "shell",
                "name": "step_04",
                "body": [
                    "genfstab -U -p /mnt >> /mnt/etc/fstab"
                ],
                "shebang": null,
                "description": "LIVE: Configuration FSTAB"
            },
            {
                "id": "6A1C02C42EC3492699B8E2954C9EFB91",
                "tag": "init",
                "type": "shell",
                "name": "step_05",
                "body": [
                    "APP=\"/mnt/opt/linuxp/\"",
                    "ROOT_CONFIG=\"/mnt/root/.config/linuxp/\"",
                    "ROO_BASHRC=\"/mnt/root/.bashrc\"",
                    "",
                    "mkdir -p \"$APP\"",
                    "mkdir -p \"$ROOT_CONFIG\"",
                    "",
                    "cp -r /opt/linuxp/* \"$APP\"",
                    "cp -r /root/.config/linuxp/* \"$ROOT_CONFIG\"",
                    "cp \"/root/.bashrc\" \"$ROO_BASHRC\""
                ],
                "shebang": null,
                "description": "LIVE: Prepare for user switching"
            },
            {
                "id": "6C5C1A13EC434063983A80CC40EAB25C",
                "tag": "init",
                "type": "shell",
                "name": "step_06",
                "body": [
                    "arch-chroot /mnt"
                ],
                "shebang": null,
                "description": "LIVE: Logging in with arch-chroot"
            },
            {
                "id": "6C5C1A13EC434063983A80CC40EAB25C",
                "tag": "init",
                "type": "shell",
                "name": "step_07",
                "body": [
                    "passwd"
                ],
                "shebang": null,
                "description": "ROOT: Changing root password"
            },
            {
                "id": "E9D32D2523AE4FC9B8DE01A3C9B43D15",
                "tag": "init",
                "type": "shell",
                "name": "step_08",
                "body": [
                    "read -p 'Username: ' username",
                    "echo $username",
                    "useradd -m -g users -G wheel,storage,power -s /bin/bash $username",
                    "passwd $username"
                ],
                "shebang": null,
                "description": "ROOT: Script to create a new user"
            },
            {
                "id": "207E199830634A86A05265CCA4D7FD6F",
                "tag": "init",
                "type": "shell",
                "name": "step_09",
                "body": [
                    "linuxp install -m package -t system --group"
                ],
                "shebang": null,
                "description": "ROOT: Installation of Useful Packages"
            },
            {
                "id": "C6B37E991C4144288EFC582305CD9D66",
                "tag": "init",
                "type": "shell",
                "name": "step_10",
                "body": [
                    "pacman -S grub efibootmgr",
                    "grub-install --target=x86_64-efi --efi-directory=/boot --bootloader-id=GRUB --recheck",
                    "grub-mkconfig -o /boot/grub/grub.cfg"
                ],
                "shebang": null,
                "description": "ROOT: Installing GRUB Settings"
            },
            {
                "id": "066F46BBF6F44F9CA69D6BB47EEE8FBB",
                "tag": "init",
                "type": "shell",
                "name": "step_11",
                "body": [
                    "systemctl enable NetworkManager",
                    "systemctl start NetworkManager"
                ],
                "shebang": null,
                "description": "ROOT: Network settings"
            },
            {
                "id": "4CEFFEE129DF4B5F979C5589BF8E9350",
                "tag": "init",
                "type": "shell",
                "name": "step_12",
                "body": [
                    "cp /etc/sudoers /etc/sudoers.bkp",
                    "echo \"root ALL=(ALL:ALL) ALL\" > /etc/sudoers",
                    "echo \"%wheel ALL=(ALL:ALL) ALL\" >> /etc/sudoers",
                    "echo \"@includedir /etc/sudoers.d\" >> /etc/sudoers"
                ],
                "shebang": null,
                "description": "ROOT: User permissions"
            },
            {
                "id": "703AD5EC282D4C0BACF2B4F9A8851ADF",
                "tag": "init",
                "type": "shell",
                "name": "step_13",
                "body": [
                    "linuxp install -m package -t dev --sudo --group",
                    "linuxp install -m package -t util --sudo --group",
                    "linuxp install -m package -t driver_vbox --sudo --group"
                ],
                "shebang": null,
                "description": "USER: Final installation of packages"
            },
            {
                "id": "703AD5EC282D4C0BACF2B4F9A8851ADF",
                "tag": "init",
                "type": "shell",
                "name": "step_14",
                "body": [
                    "linuxp install -m package -t xorg --sudo --group",
